{
  "name": "next-enterprise",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "doppler:setup:dev": "doppler setup -p aicopilotvibe -c dev_personal; doppler run --",
    "dev": "pnpm doppler:setup:dev cross-env FORCE_COLOR=1 BUILD_DIR=.next-dev next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "check:types": "pnpm tsc --noEmit",
    "prettier": "prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "analyze": "cross-env ANALYZE=true pnpm run build",
    "storybook": "cross-env FORCE_COLOR=1 storybook dev -p 6006",
    "test-storybook": "cross-env FORCE_COLOR=1 test-storybook",
    "build-storybook": "cross-env FORCE_COLOR=1 storybook build",
    "test": "cross-env FORCE_COLOR=1 vitest run",
    "test:watch": "cross-env FORCE_COLOR=1 vitest",
    "e2e:headless": "playwright test",
    "e2e:ui": "playwright test --ui",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "formatter": "pnpm run format; pnpm run prettier:fix; pnpm run lint:fix",
    "postinstall": "npx patch-package -y",
    "db:push": "pnpm doppler:setup:dev pnpm drizzle-kit push",
    "db:generate": "pnpm doppler:setup:dev pnpm drizzle-kit generate",
    "db:migrate": "pnpm doppler:setup:dev pnpm drizzle-kit migrate",
    "db:seed": "bun doppler:setup:dev bun db/seed/index.ts",
    "db:reset": "bun doppler:setup:dev bun db/reset.ts",
    "coupling-graph": "npx madge --extensions js,jsx,ts,tsx,css,md,mdx ./ --exclude '.next|.next-dev|tailwind.config.js|reset.d.ts|prettier.config.js|postcss.config.js|playwright.config.ts|next.config.js|next-env.d.ts|instrumentation.ts|e2e/|README.md|.storybook/|.eslintrc.js' --image graph.svg"
  },
